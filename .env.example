# Google Reviews Integration Configuration
# Add these to your .env.local file

# Google Places API Key
# Get this from: https://console.cloud.google.com/apis/credentials
NEXT_PUBLIC_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Google Place ID for your business
# Find this using: https://developers.google.com/maps/documentation/places/web-service/place-id
NEXT_PUBLIC_GOOGLE_PLACE_ID=your_google_place_id_here

# Optional: Enable/disable Google Reviews (set to false to use static reviews)
NEXT_PUBLIC_ENABLE_GOOGLE_REVIEWS=true
